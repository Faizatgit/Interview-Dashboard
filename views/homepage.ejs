            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mx-auto"> <!-- Use mx-auto to horizontally center the items -->
                    <!-- Interview Dashboard -->
                    <li class="nav-item">
                        <a class="nav-link" href="/interview">Interview Dashboard</a>
                    </li>

                    <!-- Video Call -->
                    <li class="nav-item">
                        <a class="nav-link" href="/interview">Video Call</a>
                    </li>
                    <!-- Quiz Compiler -->
                    <li class="nav-item">
                        <a class="nav-link" href="/quiz">Quiz</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/compiler">Compiler</a>
                    </li>
                    <!-- About Us -->
                    <li class="nav-item">
                        <a class="nav-link" href="#about">About Us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#contact">Contact</a>
                    </li>
                    <li class="nav-item">
                        <a id="btn" class="nav-link" href="http://localhost:3000/login">Logout</a>
                    </li>


                </ul>
            </div>

        </div>
    </nav>

    <!-- heading section -->

    <section class="home" id="home">
        <h1 class="hero-text">
            Welcome to <br />
            <span class="fade-in-text">INTERVIEW DASHBOARD</span>
        </h1>
    </section>

    <!-- companies section -->

    <section class="company" id="company">
        <h1 class="heading"><span>Companies</span></h1>
        <div class="flip-card-container">
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <img src="https://w7.pngwing.com/pngs/453/64/png-transparent-cisco-systems-computer-network-organization-logo-nasdaq-csco-others-computer-network-text-trademark-thumbnail.png"
                        alt="Avatar" style="width:300px;height:300px;">
                </div>
                <div class="flip-card-back">
                    <h1>Cisco</h1>
                    <p>Cisco Systems, Inc. is a leading networking company best known as a manufacturer and vendor of networking equipment. </p>
                </div>
            </div>
        </div>
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRNbwz_1hkxF6Cvb699qii3HVopVkWO6Uwbno2w0QHfCQ&s"
                        alt="Avatar" style="width:300px;height:300px;">
                </div>
                <div class="flip-card-back">
